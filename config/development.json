{
    "app": {
      "port": 3000,
      "env": "development",
      "jwtSecret": "development-jwt-secret-key",
      "baseUrl": "http://localhost:3000",
      "apiVersion": "v1"
    },
    "database": {
      "elasticsearch": {
        "host": "http://localhost:9200",
        "indexPrefix": "email_index_dev"
      },
      "redis": {
        "host": "localhost",
        "port": 6379
      }
    },
    "oauth": {
      "clientId": "development-oauth-client-id",
      "clientSecret": "development-oauth-client-secret",
      "redirectUri": "http://localhost:3000/callback"
    },
    "sync": {
      "batchSize": 50,
      "interval": 60000
    }
  }
  